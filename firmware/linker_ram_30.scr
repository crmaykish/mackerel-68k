MEMORY
{
	RAM(RX) : ORIGIN = 0x80000000, LENGTH = 0x08000000 - 0x4000 # 16KB reserved for stack
}

SECTIONS
{
    .text : { *(.text*) } > RAM
    .rodata : { *(.rodata*) } > RAM
    .bss (NOLOAD) : {
        _sbss = .;
        *(.bss*)
        *(COMMON)
        _ebss = .;
    } > RAM
    _end = .;
    
    _heap_end = ORIGIN(RAM) + LENGTH(RAM);
}
